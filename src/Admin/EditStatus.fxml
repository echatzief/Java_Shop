<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" minHeight="260.0" onKeyPressed="#addNewStatusWithKey" prefHeight="278.0" prefWidth="400.0" style="-fx-background-color: #63748e;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Admin.EditStatusController">
   <children>
      <Label layoutX="111.0" layoutY="45.0" prefHeight="39.0" prefWidth="184.0" text="Enter new status" textFill="WHITE">
         <font>
            <Font size="20.0" />
         </font>
      </Label>
      <TextField fx:id="status" layoutX="50.0" layoutY="100.0" prefHeight="30.0" prefWidth="300.0" />
      <Button fx:id="submitButton" layoutX="243.0" layoutY="165.0" mnemonicParsing="false" onMouseClicked="#addNewStatus" prefHeight="37.0" prefWidth="107.0" text="Create">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <Button fx:id="cancelBut" layoutX="50.0" layoutY="165.0" mnemonicParsing="false" onMouseClicked="#closeWindow" prefHeight="37.0" prefWidth="107.0" text="Cancel">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <StackPane layoutY="204.0" prefHeight="60.0" prefWidth="400.0">
         <children>
            <Group>
               <children>
                  <Label fx:id="warningBox" textFill="#ff4a4a">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
               </children>
               <StackPane.margin>
                  <Insets />
               </StackPane.margin>
            </Group>
         </children>
      </StackPane>
   </children>
</AnchorPane>
