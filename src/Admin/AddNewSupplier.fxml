<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.Group?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="300.0" prefWidth="400.0" style="-fx-background-color: #63748e;" xmlns="http://javafx.com/javafx/10.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Admin.AddNewSupplierController">
   <children>
      <Label layoutX="114.0" layoutY="48.0" text="This is a new supplier." textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="116.0" layoutY="81.0" text="Add supplier's phone." textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="supplierPhone" layoutX="51.0" layoutY="126.0" prefHeight="30.0" prefWidth="300.0" />
      <Button fx:id="cancelBut" layoutX="51.0" layoutY="183.0" mnemonicParsing="false" onMouseClicked="#closeWindow" prefHeight="37.0" prefWidth="107.0" text="Cancel">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
      <StackPane layoutY="222.0" prefHeight="84.0" prefWidth="400.0">
         <children>
            <Group>
               <children>
                  <Label fx:id="warningBox" textFill="#ff4a4a">
                     <font>
                        <Font size="18.0" />
                     </font>
                  </Label>
               </children>
               <StackPane.margin>
                  <Insets />
               </StackPane.margin>
            </Group>
         </children>
      </StackPane>
      <Button fx:id="submitButton" layoutX="244.0" layoutY="183.0" mnemonicParsing="false" onKeyPressed="#addProductWithKey" onMouseClicked="#addProduct" prefHeight="37.0" prefWidth="107.0" text="Create">
         <font>
            <Font size="18.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
